<p id="notice"><%= notice %></p>

<h1>Lista de estacionamientos</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parkeos.each do |parkeo| %>
      <tr>
        <td><%= parkeo.codigo %></td>
        <td><%= parkeo.description %></td>
        <td><%= link_to 'Show', parkeo %></td>
        <td><%= link_to 'Edit', edit_parkeo_path(parkeo) %></td>
        <td><%= link_to 'Destroy', parkeo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parkeo', new_parkeo_path %>
